{
  "$schema":"https://json-schema.org/draft/2020-12/schema",
  "title":"Structure (rooms)",
  "type":"object",
  "required":["rooms"],
  "properties":{
    "rooms":{
      "type":"array",
      "minItems":1,
      "items":{
        "type":"object",
        "required":["id","title","route","stylepack","element"],
        "properties":{
          "id":{"type":"string"},
          "title":{"type":"string"},
          "route":{"type":"string"},
          "stylepack":{"type":"string"},
          "element":{"type":"string"},
          "toneSet":{"type":"string"},
          "hourMap":{"type":"boolean"},
          "experiments":{
            "type":"array",
            "items":{"type":"string"}
          },
          "plaque":{"type":"string"}
        },
        "additionalProperties":false
      }
    }
  },
  "additionalProperties":false
}